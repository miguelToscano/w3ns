type ApiKey = record { value : text; owner : principal; created_at : nat64 };
type Result = variant { Ok; Err };
service : {
  get_all : () -> (vec ApiKey) query;
  has_key_registered : () -> (bool) query;
  name : () -> (text) query;
  register_key : (text) -> (Result);
  remove_key : () -> (Result);
  whoami : () -> (principal) query;
}